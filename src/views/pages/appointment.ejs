            <section>
                <div class="container">
                    <p class="lead">The following are examples of supported markup. On their own, these will not provide
                        a
                        datepicker widget; you will need to instantiate the datepicker on the markup.</p>
                    <div class="row">
                        <div class="col-md-4">
                            <h4>Click Below to Select Appointment Date</h4>
                            <!-- Datepicker -->
                            <div class="mb-3 mb-10">
                                <input id='apptDate' class="form-control required date-picker" type="text"
                                    placeholder="Appoinment Date" aria-required="true">
                            </div>

                            <h4 class="heading-title">Click Below to Select Appointment Time</h4>
                            <!-- Timepicker Step -->
                            <input type="text" class="time ui-timepicker-input" id="apptTime" autocomplete="off"
                                placeholder=" Appoinment Time" aria-required="true">
                            <!-- Timepicker Step Script -->
                            <script>
                                (function ($) {
                                    // Initialize the datepicker
                                    $('#apptTime').timepicker({
                                        'step': 15,
                                        'disableTimeRanges': [
                                            ['12:00am', '8:59am'],
                                            ['4:46pm', '11:59pm']
                                        ]
                                    });
                                })(jQuery);
                            </script>
                        </div>
                    </div>
                </div>
            </section>

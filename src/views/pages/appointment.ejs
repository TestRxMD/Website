                            </div>
                            <!-- Timepicker DisableTimeRanges Example Markup -->
                            <p><input type="text" class="time ui-timepicker-input" id="disableTimeRangesExample"
                                    autocomplete="off">
                            </p>
                            <!-- Timepicker DisableTimeRanges Example Script -->
                            <script>
                                (function ($) {
                                    $('#disableTimeRangesExample').timepicker({
